<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navigation"
    app:startDestination="@id/fragmentChoixResp">

    <fragment
        android:id="@+id/fragmentChoixResp"
        android:name="fr.eseo.carlieva.android_app.FragmentChoixResp"
        android:label="fragment_choix_resp"
        tools:layout="@layout/fragment_choix_resp" />
        <action
            android:id="@+id/action_fragmentChoixResp_to_ScrumMasterMenu"
            app:destination="@id/fragmentScrumMasterMenu" />
        <action
            android:id="@+id/action_fragmentChoixResp_to_TeamMemberMenu"
            app:destination="@id/fragmentTeamMemberMenu" />


    <fragment
        android:id="@+id/fragmentScrumMasterMenu"
        android:name="fr.eseo.carlieva.android_app.ScrumMasterMenu"
        android:label="fragment_scrum_master_menu"
        tools:layout="@layout/fragment_scrum_master_menu" />

    <fragment
        android:id="@+id/fragmentTeamMemberMenu"
        android:name="fr.eseo.carlieva.android_app.TeamMemberMenu"
        android:label="fragment_team_member_menu"
        tools:layout="@layout/fragment_team_member_menu" />

</navigation>